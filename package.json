{
  "name": "wujie-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "wujie:install": "npm i && npm-run-all -s install:*",
    "install:react-demo": "cd childApps/react-demo && npm i",
    "install:vue-demo": "cd childApps/vue-demo && npm i",
    "dev": "npm-run-all --parallel start:*",
    "start:main": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start:react-demo": "cd childApps/react-demo && npm run start",
    "start:vue-demo": "cd childApps/vue-demo && npm run dev"
  },
  "dependencies": {
    "sass": "^1.60.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "wujie-vue3": "^1.0.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "vite": "^4.1.4"
  }
}
